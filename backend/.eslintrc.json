{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "consistent-return": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-use-before-define": "off",
    "no-plusplus": "off",
    "import/prefer-default-export": "off",
    "max-len": "off",
    "no-param-reassign": "off"
  }
}